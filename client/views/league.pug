extends ./layout

block content
  .row
    .col
      if league.started
        include ./partials/leaderboard
      else
        include ./partials/draftlist
    .col-5
      include ./partials/activityFeed
    .col
      include ./partials/leagueInfo

  pre(style="font-size: 12px;")
    != helpers.dump(league)
    != helpers.dump(activity)